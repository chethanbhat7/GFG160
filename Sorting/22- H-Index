class Solution:
    def hIndex(self, citations):
        #code here
        n=len(citations)
        arr=[0]*(n+1)
        
        for i in range(n):
            if(citations[i]>=n):
                arr[n]+=1
            else:
                arr[citations[i]]+=1
        tot=0
        index=0
        for i in range(n,-1,-1):
            tot+=arr[i]
            if(tot>=i):
                return i
        return 0
