class Solution {
    public boolean searchMatrix(int[][] mat, int x) {
        int m=mat.length;
        int n=mat[0].length;
        
        for(int i=0;i<m;i++)
        for(int j=0;j<n;j++)
        {
            if(mat[i][j]==x)
            {
                return true;
            }
            if(mat[i][j]>x)
            {
                return false;
            }
        }
        return false;
    }
}
