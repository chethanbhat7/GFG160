class Solution {
    public static boolean matSearch(int mat[][], int x) {
        boolean present=false;
        
        int n=mat.length;
        int m=mat[0].length;
        
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<m;j++)
            {
                if(mat[i][j]==x)
                {
                    present=true;
                }
            }
        }
        return present;
    }
}
